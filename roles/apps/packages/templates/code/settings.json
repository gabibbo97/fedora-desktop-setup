{
  "workbench.iconTheme": "vscode-icons",
  "editor.renderLineHighlight": "all",
  "editor.roundedSelection": false,
  "editor.smoothScrolling": true,
  "editor.tabSize": 2,
  "editor.cursorBlinking": "smooth",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.formatOnPaste": true,
  "diffEditor.codeLens": true,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.enabled": false,
  "editor.suggest.localityBonus": true,
  "files.autoGuessEncoding": true,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "workbench.list.smoothScrolling": true,
  "workbench.tree.renderIndentGuides": "always",
  "workbench.view.alwaysShowHeaderActions": true,
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.labelFormat": "medium",
  "window.closeWhenEmpty": true,
  "window.dialogStyle": "custom",
  "window.title": "${dirty}${activeEditorShort}${separator}${rootName}",
  "window.titleBarStyle": "custom",
  "explorer.incrementalNaming": "smart",
  "search.smartCase": true,
  "debug.inlineValues": true,
  "scm.alwaysShowActions": true,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.experimentalUseTitleEvent": true,
  "git.allowForcePush": true,
  "git.autofetch": true,
  "git.autoStash": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.fetchOnPull": true,
  "git.rebaseWhenSync": true,
  "vsicons.dontShowNewVersionMessage": true,
  "yaml.format.singleQuote": true,
  "vsicons.presets.jsOfficial": true,
  "vsicons.presets.jsonOfficial": true,
  "vim.enableNeovim": true,
  "vim.smartRelativeLine": true,
  "workbench.colorCustomizations": {
    "statusBar.background": "#5f00af",
    "statusBar.noFolderBackground": "#5f00af",
    "statusBar.debuggingBackground": "#5f00af"
  },
  "vim.statusBarColorControl": true,
  "vim.useSystemClipboard": true
}
